{
  "name": "ezrpc",
  "version": "1.0.0",
  "description": "An isomorphic Typescript RPC system that encrypts authenticated requests from users.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --coverage",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint ./src/**/*.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "author": {
    "name" : "Ari Adair",
    "email" : "a+npm@silversupre.me"
  },
  "repository": "github:silversupreme/ezrpc",
  "license": "CC0-1.0",
  "dependencies": {
    "cross-fetch": "^3.0.6",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "fp-ts": "^2.8.2",
    "io-ts": "^2.2.10",
    "libsodium-wrappers-sumo": "^0.7.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/express-rate-limit": "^5.1.0",
    "@types/jest": "^26.0.14",
    "@types/libsodium-wrappers-sumo": "^0.7.4",
    "@types/node": "^14.11.1",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "eslint-plugin-jsdoc": "^30.5.1",
    "eslint-plugin-tsdoc": "^0.2.7",
    "jest": "^26.4.2",
    "prettier": "^2.1.2",
    "prettier-eslint": "^11.0.0",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
